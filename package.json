{
  "name": "faustwasm",
  "version": "1.0.0",
  "description": "WebAssembly version of Faust Compiler",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build-cjs": "esbuild src/index.ts --target=es2020 --bundle --sourcemap --outdir=dist --format=cjs",
    "build-esm": "esbuild src/index.ts --target=es2020 --bundle --sourcemap --outdir=dist/esm --format=esm",
    "test": "node test/node/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fr0stbyteR/faustwasm.git"
  },
  "keywords": [
    "faust",
    "webassembly"
  ],
  "author": "Grame-CNCM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fr0stbyteR/faustwasm/issues"
  },
  "homepage": "https://github.com/Fr0stbyteR/faustwasm#readme",
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/emscripten": "^1.39.5",
    "@types/node": "^16.11.7",
    "@webaudiomodules/api": "^2.0.0-alpha.1",
    "@webaudiomodules/sdk": "^0.0.5",
    "@webaudiomodules/sdk-parammgr": "^0.0.4",
    "crypto-js": "^4.1.1",
    "esbuild": "^0.13.13",
    "typescript": "^4.4.4",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1"
  }
}
